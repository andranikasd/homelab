# ============================================
# Homelab Docker Compose Environment Variables
# ============================================
# SSL is handled by Cloudflare proxy

# Traefik Dashboard & Prometheus Basic Auth
# Generate with: echo $(htpasswd -nb admin your-password) | sed -e s/\\$/\\$\\$/g
TRAEFIK_BASIC_AUTH=admin:$$apr1$$changeme$$changethis

# Grafana Admin Password
GRAFANA_ADMIN_PASSWORD=changeme-grafana-password

# N8n Configuration
N8N_ENCRYPTION_KEY=changeme-32-char-encryption-key
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=changeme-n8n-password
